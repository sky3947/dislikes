{
  "name": "Removen't Dislikes",
  "description": "Brings back the dislike count on YouTube.",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/watch?v=*"],
      "js": [
        "content-script.js",
        "options.js"
      ]
    }
  ]
}